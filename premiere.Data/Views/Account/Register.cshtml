@model premiere.Data.Models.RegisterModel
@{
    ViewBag.Title = "Register";
}

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
    <h2>Create a new account.</h2>
</hgroup>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

   
                @Html.LabelFor(m => m.UserName)
                @Html.TextBoxFor(m => m.UserName ,new { @type="text", @class="form-control" ,@placeholder="User name",@required="",@autofocus=""})
           
                @Html.LabelFor(m => m.Password)
                @Html.PasswordFor(m => m.Password,new { @type="password", @class="form-control" ,@placeholder="Password",@required="",@autofocus=""})
           
                @Html.LabelFor(m => m.ConfirmPassword)
                @Html.PasswordFor(m => m.ConfirmPassword,new { @type="password", @class="form-control" ,@placeholder="Password",@required="",@autofocus=""})
          
                @Html.LabelFor(m => m.Email)
                @Html.TextBoxFor(m => m.Email,new { @type="email", @class="form-control" ,@placeholder="User name",@required="",@autofocus=""})
           
                @Html.LabelFor(m => m.Facebook)
                @Html.TextBoxFor(m => m.Facebook,new { @type="text", @class="form-control" ,@placeholder="Fackbook",@required="",@autofocus=""})
           
                @Html.LabelFor(m => m.Age)
                @Html.TextBoxFor(m => m.Age,new { @type="text", @class="form-control" ,@placeholder="Age",@required="",@autofocus=""})
           
                @Html.LabelFor(m => m.Rate)
                @Html.TextBoxFor(m => m.Rate,new { @type="text", @class="form-control" ,@placeholder="Rate",@required="",@autofocus=""})
         
                @Html.LabelFor(m => m.LastName)
                @Html.TextBoxFor(m => m.LastName,new { @type="text", @class="form-control" ,@placeholder="Last name",@required="",@autofocus=""})
         
                @Html.LabelFor(m => m.FirstName)
                @Html.TextBoxFor(m => m.FirstName,new { @type="text", @class="form-control" ,@placeholder="First name",@required="",@autofocus=""})
       

        <input type="submit" value="Register" />
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
